{
    "JobProcessorMessageQueueName": "DefaultRabbitMQ",
    "JobTriggerRepositoryName": "ExampleMongo",
    "WebApi": {
        "HostName": "localhost",
        "Port": 9001,
        "Origins": [ "*" ]
    },
    "NLogAdapter": {
        "ConfigType": "File",
        "ConfigFile": "IntegrationEngine.nlog.xml"
    },
    "IntegrationPoints": {
        "Mail": [
            {
                "IntegrationPointName": "DefaultMail",
                "HostName": "localhost",
                "Port": 25
            },
            {
                "IntegrationPointName": "FooMailClient",
                "HostName": "localhost",
                "Port": 25
            }
        ],
        "RabbitMQ": [
            {
                "IntegrationPointName": "DefaultRabbitMQ",
                "QueueName": "vagrant",
                "ExchangeName": "amq.direct",
                "UserName": "vagrant",
                "Password": "vagrant",
                "HostName": "localhost",
                "VirtualHost": "/"
            }
        ],
        "Elasticsearch": [
            {
                "IntegrationPointName": "DefaultElasticsearch",
                "Protocol": "http",
                "HostName": "localhost",
                "Port": 9200,
                "DefaultIndex": "integration-engine"
            }
        ],
        "JsonService": [
            {
                "IntegrationPointName": "ExampleJsonService",
                "BaseUri": "http://example.com",
                "UserName": "inengine",
                "Password": "secret",
                "AlwaysSendBasicAuthHeader": true
            }
        ],
        "Mongo": [
            {
                "IntegrationPointName": "ExampleMongo",
                "HostName": "localhost"
            }
        ]
    }
}
